<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/horizontal_margin"
    android:background="@drawable/bg_diet_details"
    android:layout_marginHorizontal="16dp"
    android:orientation="vertical"
    >


    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/item_diet_details_tv_month"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="left"
        android:layout_marginTop="@dimen/vertical_margin"
        android:text="11월 21일"
        android:textSize="16dp"
        android:fontFamily="@font/tmoney_round_wind_extra_bold_font"
        />

    <androidx.appcompat.widget.LinearLayoutCompat
        android:id="@+id/item_diet_details_layout_breakfast"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginTop="@dimen/vertical_margin_large"
        >

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/item_diet_details_tv_breakfast_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="아침"
            android:textSize="@dimen/text_large"
            android:textColor="@color/black"
            android:fontFamily="@font/spoqa_han_sans_neo_bold"
            />

        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="4dp"
            >
            <CheckBox
                android:id="@+id/item_diet_details_chb_breakfast_rice"
                android:layout_width="wrap_content"
                android:layout_height="20dp"/>
            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/item_diet_details_tv_breakfast_content_rice"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="흰 쌀밥"
                android:textColor="@color/subGrey"
                android:fontFamily="@font/spoqa_han_sans_neo_regular"
                />

        </androidx.appcompat.widget.LinearLayoutCompat>



        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="2dp"

            >
            <CheckBox
                android:id="@+id/item_diet_details_chb_breakfast"
                android:layout_width="wrap_content"
                android:layout_height="20dp"/>
            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/item_diet_details_tv_breakfast_content"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="닭가슴살"
                android:textColor="@color/subGrey"
                android:fontFamily="@font/spoqa_han_sans_neo_regular"
                />

        </androidx.appcompat.widget.LinearLayoutCompat>




    </androidx.appcompat.widget.LinearLayoutCompat>



    <androidx.appcompat.widget.LinearLayoutCompat
        android:id="@+id/item_diet_details_layout_lunch"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginTop="@dimen/vertical_margin_large"
        >



        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/item_diet_details_tv_lunch_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="점심"
            android:textSize="@dimen/text_large"
            android:textColor="@color/black"
            android:fontFamily="@font/spoqa_han_sans_neo_bold"

            />

        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="4dp"

            >
            <CheckBox
                android:id="@+id/item_diet_details_chb_lunch_rice"
                android:layout_width="wrap_content"
                android:layout_height="20dp"/>

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/item_diet_details_tv_lunch_content_rice"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="흰 쌀밥"
                android:textColor="@color/subGrey"
                android:fontFamily="@font/spoqa_han_sans_neo_regular"
                />

        </androidx.appcompat.widget.LinearLayoutCompat>

        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="2dp"

            >
            <CheckBox
                android:id="@+id/item_diet_details_chb_lunch"
                android:layout_width="wrap_content"
                android:layout_height="20dp"/>

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/item_diet_details_tv_lunch_content"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="연어스테이크"
                android:textColor="@color/subGrey"
                android:fontFamily="@font/spoqa_han_sans_neo_regular"
                />

        </androidx.appcompat.widget.LinearLayoutCompat>




    </androidx.appcompat.widget.LinearLayoutCompat>






    <androidx.appcompat.widget.LinearLayoutCompat
        android:id="@+id/item_diet_details_layout_dinner"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginTop="@dimen/vertical_margin_large"
        >


        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/item_diet_details_tv_dinner_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="저녁"
            android:textSize="@dimen/text_large"
            android:textColor="@color/black"
            android:fontFamily="@font/spoqa_han_sans_neo_bold"
            />

        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="4dp"

            >

            <CheckBox
                android:id="@+id/item_diet_details_chb_dinner_rice"
                android:layout_width="wrap_content"
                android:layout_height="20dp"/>
            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/item_diet_details_tv_dinner_content_rice"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="흰 쌀밥"
                android:textColor="@color/subGrey"
                android:fontFamily="@font/spoqa_han_sans_neo_regular"
                />


        </androidx.appcompat.widget.LinearLayoutCompat>



        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="2dp"

            >

            <CheckBox
                android:id="@+id/item_diet_details_chb_dinner"
                android:layout_width="wrap_content"
                android:layout_height="20dp"/>
            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/item_diet_details_tv_dinner_content"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="파스타 밀키트"
                android:textColor="@color/subGrey"
                android:fontFamily="@font/spoqa_han_sans_neo_regular"
                />


        </androidx.appcompat.widget.LinearLayoutCompat>



    </androidx.appcompat.widget.LinearLayoutCompat>



    <androidx.appcompat.widget.LinearLayoutCompat
        android:id="@+id/item_diet_details_layout_snack_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginTop="@dimen/vertical_margin_large"
        >


        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/item_diet_details_tv_snack_1_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="간식1"
            android:textSize="@dimen/text_large"
            android:textColor="@color/black"
            android:fontFamily="@font/spoqa_han_sans_neo_bold"
            />

        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="4dp"

            >

            <CheckBox
                android:id="@+id/item_diet_details_chb_snack_1"
                android:layout_width="wrap_content"
                android:layout_height="20dp"/>
            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/item_diet_details_tv_snack_1_content"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="파스타 밀키트"
                android:textColor="@color/subGrey"
                android:fontFamily="@font/spoqa_han_sans_neo_regular"
                />


        </androidx.appcompat.widget.LinearLayoutCompat>



    </androidx.appcompat.widget.LinearLayoutCompat>



    <androidx.appcompat.widget.LinearLayoutCompat
        android:id="@+id/item_diet_details_layout_snack_2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginTop="@dimen/vertical_margin_large"
        >


        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/item_diet_details_tv_snack_2_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="간식2"
            android:textSize="@dimen/text_large"
            android:textColor="@color/black"
            android:fontFamily="@font/spoqa_han_sans_neo_bold"
            />

        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="4dp"

            >

            <CheckBox
                android:id="@+id/item_diet_details_chb_snack_2"
                android:layout_width="wrap_content"
                android:layout_height="20dp"/>
            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/item_diet_details_tv_snack_2_content"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="파스타 밀키트"
                android:textColor="@color/subGrey"
                android:fontFamily="@font/spoqa_han_sans_neo_regular"
                />


        </androidx.appcompat.widget.LinearLayoutCompat>



    </androidx.appcompat.widget.LinearLayoutCompat>


</androidx.appcompat.widget.LinearLayoutCompat>