<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="hansung.ac.kr">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/baekgu_mipmap"
        android:label="FinalProject"
        android:roundIcon="@drawable/baekgu_mipmap"
        android:supportsRtl="true"
        android:theme="@style/Theme.FinalProject"
        tools:targetApi="31">
        <activity
            android:name=".src.splash.SplashActivity"
            android:exported="true"
            tools:ignore="Instantiatable">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".src.login.LoginActivity"
            android:exported="true"
            android:screenOrientation="portrait"
            tools:ignore="Instantiatable" />

        <activity
            android:name=".src.signup.SignupActivity"
            android:exported="true"
            android:screenOrientation="portrait"
            tools:ignore="Instantiatable"
            />


        <activity
            android:name=".src.MainActivity"
            android:exported="true"
            android:screenOrientation="portrait"
            />

        <activity
            android:name=".src.survey.SurveyActivity"
            tools:ignore="Instantiatable"
            android:exported="true"
            android:screenOrientation="portrait"
            />

        <activity
            android:name=".src.diet_details.DietDetailsActivity"
            tools:ignore="Instantiatable"
            android:exported="true"
            android:screenOrientation="portrait"
            />

        <activity
            android:name=".src.profile_edit.ProfileEditActivity"
            tools:ignore="Instantiatable"
            android:exported="true"
            android:screenOrientation="portrait"
            />

        <activity
            android:name=".src.product_details.ProductDetailsActivity"
            android:exported="true"
            android:screenOrientation="portrait"
            />



    </application>

</manifest>